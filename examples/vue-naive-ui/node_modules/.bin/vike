#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules/vike/node/cli/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules/vike/node/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules/vike/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules/vike/node/cli/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules/vike/node/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules/vike/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/vike@0.4.194_vite@5.4.3/node_modules:/home/jessiea/projects/vike-vue-naive-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vike/node/cli/bin-entry.js" "$@"
else
  exec node  "$basedir/../vike/node/cli/bin-entry.js" "$@"
fi
